<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
  <config>
    <!-- 使用 .NET SDK 中的 NuGet -->
    <add key="useDotnetNuGet" value="true" />
    <!-- 禁用包缓存（可选） -->
    <add key="globalPackagesFolder" value="packages" />
  </config>
</configuration>